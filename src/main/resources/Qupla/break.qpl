// returns val

// this function can be used to set a breakpoint in the Qupla code.
// the interpreter will intecept usage of break to achieve this
// You can set a breakpoint in the Java debugger at the corresponding
// location in QuplaEvalContext.interceptCall


template break<T> {
  func T break<T> (T val) {
    return val
  }
}

use break<Trit>, <Tryte>, <Tiny>, <Int>, <Huge>, <Hash>
use break<Tryte2>, <Tiny2>, <Tiny4>, <Int2>, <Huge2>, <Hash2>
