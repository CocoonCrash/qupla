// returns lhs < rhs ? lhs : rhs

template min<T> {
  func T min<T> (T lhs, T rhs) {
    return isLess[cmp<T>(lhs, rhs)] ? lhs : rhs
  }
}


test -2 = min<Tiny>(-1, -2)
test -2 = min<Tiny>(-2, -1)
test -1 = min<Tiny>(-1, 0)
test -1 = min<Tiny>(0, -1)
test -1 = min<Tiny>(-1, 1)
test -1 = min<Tiny>(1, -1)
test 0 = min<Tiny>(1, 0)
test 0 = min<Tiny>(0, 1)
test 1 = min<Tiny>(1, 2)
test 1 = min<Tiny>(2, 1)
